<!DOCTYPE html> 
<html> 
    <head> 
        <title> dashboard test</title>
        <meta charset='utf-8' > 
        <link rel='stylesheet' href="/stylesheets/dashboardEleitor.css"/> 
        
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    </head>
    <body> 
        <header> 
            <div class='logo-icon'> 
               <span><h3>e-voto</h3></span> 
            </div>

            <div class='user-logo'>
                <i class='fas fa-user'> <span>adm</span></i>
            </div>
        </header>


    

        <div class='side-bar'> 
            <a href="/dashboard"><i class="fas fa-user-edit"></i><span>Gerenciar candidato</span></a>
            <a href="/dashboard_eleitor"><i class="fas fa-user-edit"></i><span>Gerenciar eleitor</span></a>
            <a href="#"><i class="fas fa-chart-bar"></i><span>Relat√≥rios</span></a>
            <a href="/login"><i class="fas fa-sign-out-alt"></i><span>Logout</span></a>
        </div>


        <div class='content'>
            
            <div class='content-card'>
                
                <div class='content-header'>
                    <span><h3>Eleitores </h3></span>

                    
                </div>

                <div class='table-header'> 
                    <div class='table-headers'>ID</div>  
                    <div class='table-headers'>NOME</div>
                    <div class='table-headers'>CPF</div>
                    <div class='table-headers'>EMAIL</div>
                    <div class='table-headers'>TITULO</div>
                        
                         
                </div>
                

                <% eleitores.forEach(function(eleitor){ %>

                    <div class="data-row">
    
                        <div class="data-row-2"> <%= eleitor.dataValues.id%> </div>
                        <div class="data-row-2"> <%= eleitor.dataValues.nome%> </div>
                        <div class="data-row-2"> <%= eleitor.dataValues.cpf%> </div>
                        <div class="data-row-2"> <%= eleitor.dataValues.email%> </div>
                        <div class="data-row-2"> <%= eleitor.dataValues.titulo%> </div>
                        
        
                    </div>
                  
                <% }); %>

               
            
            </div>

        </div>

       

  
 

        <script src="/js/dashboardHome.js"> </script>
    </body>
</html>